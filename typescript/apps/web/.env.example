# Database
DATABASE_URL=postgresql://user:password@localhost:5432/health_db

# API Server URL (for BetterAuth and VAPI proxy)
# In development: http://localhost:3001
# In production: your Cloud Run URL (e.g., https://health-api-xxx.run.app)
VITE_API_URL=http://localhost:3001

# OAuth Providers
# Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Apple OAuth
APPLE_CLIENT_ID=your_apple_client_id_here
APPLE_CLIENT_SECRET=your_apple_client_secret_here

# Microsoft OAuth
MICROSOFT_CLIENT_ID=your_microsoft_client_id_here
MICROSOFT_CLIENT_SECRET=your_microsoft_client_secret_here

# Passkey Configuration
PASSKEY_RP_ID=localhost
PASSKEY_RP_NAME=Health Application
PASSKEY_ORIGIN=http://localhost:5174

# Email Service (for OTP and verification emails)
# Configure your email provider here
# SMTP_HOST=smtp.example.com
# SMTP_PORT=587
# SMTP_USER=your_email@example.com
# SMTP_PASS=your_email_password
# SMTP_FROM=noreply@example.com

# VAPI AI Agent Configuration
# Get your keys from https://dashboard.vapi.ai/
# NOTE: The private API key (VAPI_API_KEY) should be set in the API server, not here
# Public key for web SDK calls (get from VAPI dashboard > Account > Public Key)
VITE_VAPI_PUBLIC_KEY=your_vapi_public_key_here
